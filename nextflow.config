profiles {
  standard {
    docker {
      enabled = true
      fixOwnership = true
      runOptions = "-u \$(id -u):\$(id -g)"
    }
    process {
      cpus = 1

      $msgfPlus {
        cpus = 4
      }
      $percolator {
        cpus = 4
      }
    }
  }
  slurm {
    docker {
      enabled = true
      fixOwnership = true
      runOptions = "-u \$(id -u):\$(id -g)"
    }
    process {
      executor = 'slurm'
      cpus = 1

      $msgfPlus {
        cpus = 4
      }
      $percolator {
        cpus = 4
      }
    }
  }
}
